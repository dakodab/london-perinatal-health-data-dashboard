  "author": "",
  "license": "ISC",
  "description": "Interactive multimedia dashboard exploring perinatal care in London with borough-level data and visualizations.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dakodab/london-perinatal-health-data-dashboard.git"
  }, 
  "bugs": {
    "url": "https://github.com/dakodab/london-perinatal-health-data-dashboard/issues"
  },
  "homepage": "https://github.com/dakodab/london-perinatal-health-data-dashboard#readme",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "csv-parser": "^3.2.0",
    "mysql2": "^3.14.0"
  }
}
